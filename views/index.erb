<% @students.each do |student| %>
  <p>First Name: <%= student.first_name().capitalize %></p>
  <p>Last Name: <%= student.last_name() .capitalize%></p>
  <!-- <p>House: <%= student.house_id() %></p> -->
    <p>House: <%= student.house.name.capitalize %></p>
  <p>Age: <%= student.age() %></p>
  <p><a href="/students/<%= student.id %>">Student Admin</a></p>
  <hr />
<% end %>
